            </ul>
        </div><!-
    </nav>
</aside><!-- /#left-panel -->
<!-- Left Panel -->
<!-- Right Panel -->

<div id="right-panel" class="right-panel">

    <!-- Header-->
    <header id="header" class="header">

        <div class="header-menu">

            <div class="col-sm-7">
                <a id="menuToggle" class="menutoggle pull-left"><i class="fa fa fa-tasks"></i></a>
                <div class="header-left">
                    <h1>Liste des étudiantes</h1>

                </div>
            </div>

            <div class="col-sm-5">
                <div class="user-area dropdown float-right">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        <img class="user-avatar rounded-circle" src="/Content/myimage/admin.jpg" alt="User Avatar">
                    </a>

                    <div class="user-menu dropdown-menu">
                        <a class="nav-link" href="#"><i class="fa fa-user"></i> My Profile</a>

                        <a class="nav-link" href="#"><i class="fa fa-user"></i> Notifications <span class="count">13</span></a>

                        <a class="nav-link" href="#"><i class="fa fa-cog"></i> Settings</a>

                        <a class="nav-link" href="#"><i class="fa fa-power-off"></i> Logout</a>
                    </div>
                </div>

                <div class="language-select dropdown" id="language-select">
                    <a class="dropdown-toggle" href="#" data-toggle="dropdown" id="language" aria-haspopup="true" aria-expanded="true">
                        <i class="flag-icon flag-icon-us"></i>
                    </a>
                    <div class="dropdown-menu" aria-labelledby="language">
                        <div class="dropdown-item">
                            <span class="flag-icon flag-icon-fr"></span>
                        </div>
                        <div class="dropdown-item">
                            <i class="flag-icon flag-icon-es"></i>
                        </div>
                        <div class="dropdown-item">
                            <i class="flag-icon flag-icon-us"></i>
                        </div>
                        <div class="dropdown-item">
                            <i class="flag-icon flag-icon-it"></i>
                        </div>
                    </div>
                </div>

            </div>
        </div>

    </header><!-- /header -->
    <!-- Header-->



    <div class="content mt-3">
        <div class="animated fadeIn">
            <div class="row">

                <div class="col-md-12">
                    <div class="card">
                        <div class="card-header">
                            <center><input type="text" id="search" placeholder="search" class="form-control" /></center>

                            <script src="~/Scripts/jquery-3.3.1.min.js"></script>
                            <script>
                                $(document).ready(function () {
                                    function Contains(text_one, text_two) {
                                        if (text_one.indexOf(text_two) != -1)
                                            return true;
                                    }
                                    $("#search").keyup(function () {
                                        var searchText = $("#search").val().toLowerCase();
                                        $(".search").each(function () {
                                            if (!Contains($(this).text().toLowerCase(), searchText)) {
                                                $(this).hide();
                                            }
                                            else {
                                                $(this).show();
                                            }
                                        });
                                    });
                                });
                            </script>
                        </div>
                        <div class="card-body">
                            <table id="bootstrap-data-table-export" class="table table-striped table-bordered">
                                <tr>

                                    <th>
                                        Image
                                    </th>
                                    <th class="th-sm">
                                        Gestion des etudiantes
                                    </th>
                                    <th class="th-sm">
                                        Nom
                                    </th>
                                    <th>
                                        Prenom
                                    </th>
                                    <th class="th-sm">
                                        Date_Naissance
                                    </th>
                                    <th>
                                        Lieu de Naissance
                                    </th>

                                    <th>
                                        Ville
                                    </th>
                                    <th class="th-sm">
                                        Telephone
                                    </th>

                                    <th>
                                        Numéro de carte Nationale
                                    </th>
                                    <th>
                                        Email
                                    </th>

                                    <th>
                                        Numéro National d'etudiant
                                    </th>
                                    <th>
                                        Type de Bac
                                    </th>
                                    <th>
                                        Annee de Bac
                                    </th>
                                    <th>
                                        Mention du Bac
                                    </th>
                                    <th>
                                        Diplome
                                    </th>
                                    <th>
                                        Ecole
                                    </th>

                                    <th>
                                        redouble_1ere_annee
                                    </th>
                                    <th>
                                        redouble_2eme_annee
                                    </th>
                                    <th>
                                        redouble_3eme_annee
                                    </th>
                                    <th>
                                        Note S1
                                    </th>
                                    <th>
                                        Note S2
                                    </th>
                                    <th>
                                        Note S3
                                    </th>
                                    <th>
                                        Note S4
                                    </th>
                                    <th>
                                        Note S5
                                    </th>
                                    <th>
                                        Note S6
                                    </th>


                                    <th>
                                        Note de Score
                                    </th>

                                    <th>
                                        Filiére choisie
                                    </th>

                                    <th></th>
                                </tr>

                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>
                                            @{
                                                var base64 = Convert.ToBase64String(item.photo_identite);
                                                var images = string.Format("data:image/gif;base64,{0}", base64);
                                            }
                                            <img src="@images" height="100" width="100" />
                                        </td>


                                        <td>

                                            <button class="btn btn-primary btn-sm">
                                                <i class="fa fa-dot-circle-o"> </i>  @Html.ActionLink("Details", "Details", new { id = item.id })
                                            </button>

                                            <br /><br />
                                            <button class="btn btn-danger btn-sm">
                                                <i class="fa fa-dot-circle-o"> </i>  @Html.ActionLink("Delete", "Delete", new { id = item.id })
                                            </button>


                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.nom)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.prnom)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.date_naissance)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.lieu_naissance)
                                        </td>

                                        <td>
                                            @Html.DisplayFor(modelItem => item.ville)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.tel)
                                        </td>

                                        <td>
                                            @Html.DisplayFor(modelItem => item.cin)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.email)
                                        </td>

                                        <td>
                                            @Html.DisplayFor(modelItem => item.cne)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.type_bac)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.annee_bac)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.mention_bac)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.diplome)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.ecole)
                                        </td>

                                        <td>
                                            @Html.DisplayFor(modelItem => item.redouble_1ere_annee)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.redouble_2eme_annee)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.redouble_3eme_annee)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.note_S1)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.note_S2)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.note_S3)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.note_S4)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.note_S5)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.note_S6)
                                        </td>


                                        <td>
                                            @Html.DisplayFor(modelItem => item.score)
                                        </td>

                                        <td>
                                            @Html.DisplayFor(modelItem => item.filiere_choisie)
                                        </td>

                                    </tr>
                                }

                            </table>




                        </div>
                    </div>
                </div>


            </div>
        </div><!-- .animated -->
    </div><!-- .content -->
</div>


